<Page x:Class="WpfTest2.MainPages.Navigation1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfTest2.MainPages"
      mc:Ignorable="d" 
      d:DesignHeight="400" d:DesignWidth="320"
      Background="White"
      Title="Navigation1">
    <Page.Resources>
        <Style TargetType="TextBlock">
            <Setter Property="FontSize" Value="30"></Setter>
        </Style>
    </Page.Resources>
    <Grid Width="320" Height="400">
        <StackPanel>
            <TextBlock Text="AAAAA"></TextBlock>
            <TextBlock Text="BBBBB"></TextBlock>
            <TextBlock Text="ccccc"></TextBlock>
            <TextBlock Text="EEEEE"></TextBlock>
            <TextBlock Text="DDDDD"></TextBlock>
            <TextBlock Text="FFFFFF"></TextBlock>
        </StackPanel>
    </Grid>
</Page>
